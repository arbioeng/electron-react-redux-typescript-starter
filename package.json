{
    "name": "electron-react-redux-typescript",
    "version": "0.0.0",
    "main": "./dist/main.js",
    "scripts": {
        "dev-main": "cross-env NODE_ENV=development nodemon",
        "dev-renderer": "cross-env NODE_ENV=development webpack serve --config webpack.renderer.config.js --mode development",
        "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.config.js --mode production",
        "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.config.js --mode production",
        "build": "npm run build-main && npm run build-renderer",
        "pack": "npm run build && electron-builder --dir",
        "dist": "npm run build && electron-builder",
        "lint": "eslint --ext=jsx,js,tsx,ts ./src"
    },
    "build": {
        "directories": {
            "output": "dist"
        },
        "files": [
            "dist/",
            "node_modules/",
            "package.json"
        ]
    },
    "author": {
        "name": "test",
        "email": "test@test.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/qmonk/electron-react-redux-typescript-starter"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.12.7",
        "@babel/preset-env": "^7.12.7",
        "@babel/preset-react": "^7.12.7",
        "@babel/preset-typescript": "^7.12.7",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@typescript-eslint/eslint-plugin": "^4.8.1",
        "@typescript-eslint/parser": "^4.8.1",
        "babel-loader": "^8.2.1",
        "cross-env": "^7.0.2",
        "electron": "^11.0.2",
        "electron-builder": "^22.9.1",
        "eslint": "^7.14.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4.2.0",
        "fork-ts-checker-webpack-plugin": "^6.0.3",
        "html-webpack-plugin": "^4.5.0",
        "nodemon": "^2.0.6",
        "prettier": "^2.2.0",
        "typescript": "^4.1.2",
        "webpack": "^5.6.0",
        "webpack-cli": "^4.2.0",
        "webpack-dev-server": "^3.11.0"
    },
    "dependencies": {
        "@babel/plugin-transform-runtime": "^7.12.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1"
    }
}
